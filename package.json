{
  "name": "sd_project",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@heroicons/react": "^2.1.3",
    "@mui/material": "^5.15.17",
    "@mui/x-data-grid": "^7.3.0",
    "@react-oauth/google": "^0.12.1",
    "@testing-library/user-event": "^13.5.0",
    "auth0-lock": "^12.5.0",
    "bootstrap": "^5.3.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "google-auth-library": "^9.7.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "jwt-decode": "^4.0.0",
    "mssql": "^10.0.2",
    "node-fetch": "^3.3.2",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^17.0.2",
    "react-headroom": "^3.2.1",
    "react-plotly.js": "^2.6.0",
    "react-router": "^6.22.3",
    "react-router-dom": "^5.3.4",
    "react-scripts": "5.0.1",
    "react-to-pdf": "^1.0.1",
    "react-use-googlelogin": "^0.12.5",
    "styled-components": "^6.1.8",
    "swiper": "^11.1.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "jest --coverage && type .\\coverage\\lcov-report\\lcov.info | coveralls",
    "eject": "react-scripts eject"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "lines": 30
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/App.js",
      "!<rootDir>/src/index.js",
      "!<rootDir>/src/pages/HRHome/HRHome.js",
      "!<rootDir>/src/pages/LandingNew/LandingNew.js",
      "!<rootDir>/src/pages/StaffDashboard/*",
      "!<rootDir>/src/components/Carousel/*",
      "!<rootDir>/src/components/HRdatagrid/*",
      "!<rootDir>/src/components/Loader/Loader.js",
      "!<rootDir>/src/components/Modal/Modal.js",
      "!<rootDir>/src/components/Reporting/Reporting.js",
      "!<rootDir>/src/components/StaffCarWash/StaffCarWash.js",
      "!<rootDir>/src/components/StaffHeader/StaffHeader.js",
      "!<rootDir>/src/reportWebVitals*",
      "!<rootDir>/src/setupTests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css|less|PNG)$": "<rootDir>/public/__mocks__/fileMock.js",
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/public/__mocks__/styleMock.js"
    },
    "testEnvironment": "jsdom",
    "coverageReporters": [
      "text",
      "html"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@types/testing-library__react": "^10.2.0",
    "jest": "^27.5.1",
    "jest-fetch-mock": "^3.0.3",
    "path-browserify": "^1.0.1"
  }
}
